{
  "name": "nocli",
  "version": "0.1.1",
  "description": "tiny utility to run a node module via the cli",
  "main": "nocli.js",
  "type": "commonjs",
  "bin": {
    "nocli": "./nocli.js"
  },
  "scripts": {
    "lint": "otr lint",
    "format": "otr format",
    "test": "otr test",
    "release": "otr release",
    "checks": "yarn lint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/justinvdm/nocli.git"
  },
  "keywords": [
    "cli"
  ],
  "author": "justinvdm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justinvdm/nocli/issues"
  },
  "homepage": "https://github.com/justinvdm/nocli",
  "oftherivier": {
    "type": "vanilla",
    "src": "src/index.js"
  },
  "renovate": {
    "extends": [
      "github>oftherivier/tools"
    ]
  },
  "devDependencies": {
    "@oftherivier/tools": "0.1.35",
    "shelljs": "0.8.4"
  },
  "dependencies": {
    "js-yaml": "^3.5.3",
    "lodash": "^4.5.1",
    "require-relative": "^0.8.7",
    "yargs": "^15.3.1"
  }
}
